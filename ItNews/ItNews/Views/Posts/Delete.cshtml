@model NewsItem

@{
    ViewData["Title"] = "Delete";
}
<h2>Are you sure you want to delete this list ?</h2>
<form asp-action="Delete" method="post">
    <input type="submit" class="btn btn-outline-danger" value="Yes, Delete" />
    <a href="javascript:history.go(-1)"><button type="button" class="btn btn-outline-danger btn-sm">Cancel</button></a>
</form>

